{
  "name": "todo-list",
  "version": "1.0.0",
  "description": "Following https://watchandcode.com/ practical-javascript",
  "main": "todoList.js",
  "dependencies": {},
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run coverage",
      "pre-push": "npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paulmboyce/todos.git"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "scripts": {
    "test": "jest",
    "watch:main": "jest --watch *.js",
    "watch:src": "jest --watch src/*.js",
    "watch": "npm run watch:main & npm run watch:src",
    "coverage": "jest --coverage",
    "lint:main": "eslint *.js --fix",
    "lint:src": "eslint src/*.js --fix",
    "lint": "npm run lint:main & npm run lint:src",
    "clean": "rm dist/bundle.js",
    "build:dev": "webpack --config webpack.DEV.config.js",
    "start:dev": "webpack-dev-server  --config webpack.DEV.config.js"
  },
  "author": "",
  "license": "ISC"
}
